using System.Text;
using System.Threading.Tasks;

namespace ConsoleApplication18
{
    class Program
    {
        static void Main(string[] args)
        {
            int a = 1;
            for (int b = 1; b < 15;)
            {
                int sum = a + b;
                a = sum;
                a++;
              
               
                
                Console.WriteLine(a);
                
            }
            Console.ReadKey();
        }
    }
}
